<script>
 $(document).ready(function () {
 $("button").click(function () {
 var links=$('li img');
 links.each(function (i, img) {
		
 if(i == 0)
		{
 $(this).animate({ top: '130px'}, 'slow'); 
		}
 if(i == 2)
		{
 $(this).animate({ top: '335px'}, 'slow'); 
		}
 if(i == 4)
		{
 $(this).animate({ top: '535px'}, 'slow'); 
		}
		
		});
			
	});
		
});
</script>


