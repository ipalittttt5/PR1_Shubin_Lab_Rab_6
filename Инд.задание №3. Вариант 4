<!DOCTYPE html> 
<html> 
<head> 
<script src="http://code.jquery.com/jquery-latest.js"> 
</script> 
 
<script> 
    
     $(document).ready(function () { 
        $("Button").click(function () { 									
			for (var j = 1; j <= 10; ++j) {
				for (var i = 5; i >= 1; --i) {
					$("#div" + i).fadeIn(3000);								
				}
				for (var i = 5; i >= 1; --i) {			
					$("#div" + i).fadeOut(3000);				
				}			
			}
        }); 		
    }); 		
		
    
</script> 
</head> 
 
<body> 

	<button>Button</button> 

	<br><br> 	
		<div id="div1" style="display:none;height:40px;width:40px;background-color:red"> 1 </div>
		<div id="div2" style="display:none;height:40px;width:40px"> 2 </div>
		<div id="div3" style="display:none;height:40px;width:40px;background-color:green"> 3 </div>
		<div id="div4" style="display:none;height:40px;width:40px;background-color:blue"> 4 </div>
		<div id="div5" style="display:none;height:40px;width:40px;background-color:yellow"> 5 </div>
</body> 
</html>
